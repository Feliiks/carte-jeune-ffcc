generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id                Int     @id @default(autoincrement())
  firstname         String  @db.VarChar(50)
  lastname          String  @db.VarChar(50)
  email             String  @db.VarChar(100)
  password          String  @db.VarChar(100)
  is_email_verified Boolean @default(false)
  email_token       String? @db.VarChar(255)
  password_token    String? @db.VarChar(255)
}

model youth_card_requests {
  id             Int      @id @default(autoincrement())
  user_id        Int      @unique(map: "youth_card_requests_user_id_uindex")
  user_tel       String   @db.VarChar(20)
  user_birthdate DateTime @db.Date
  user_student   Boolean  @default(false)
}
